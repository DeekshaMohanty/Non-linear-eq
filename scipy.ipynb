    "import scipy.optimize\n",
    "import numpy as np\n",
    "\n",
    "A = np.array([[1, 2, -1], [-2, 0, 1], [1, -1, 0]])\n",
    "\n",
    "def foo(X):\n",
    "    X = np.array(X)\n",
    "    b = np.array([2, 1, 1])\n",
    "    out = np.dot(A, X) - b\n",
    "    return out\n",
    "\n",
    "sol = scipy.optimize.root(foo, [0, 0, 0])  #initial guess for the roots\n",
    "print(sol)"
   ]
